{
  "name": "rm2back",
  "version": "1.0.0",
  "description": "backend for project ramverk2",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "chmod 777 db && nyc --reporter=html --reporter=text mocha test/**/*.js",
    "test-scrutinizer": "nyc --all --reporter=clover mocha test/**/*.js",
    "report-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "report-codecov": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "start": "node app.js",
    "production": "NODE_ENV='production' node app.js"
  },
  "author": "KW",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "morgan": "^1.9.1",
    "sqlite3": "^4.0.6",
    "ws": "^6.2.0"
  },
  "devDependencies": {
    "codecov": "^3.2.0",
    "coveralls": "^3.0.3",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0"
},
"repository": {
    "type": "git",
    "url": "git+https://github.com/klaswarna/rm2back.git"
  }
}
